{
  "name": "vane-cli",
  "version": "1.1.0",
  "description": "",
  "main": "/dist/index.js",
  "bin": {
    "vane": "./bin/cli.js"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "eslint": "eslint --fix src --ext .ts --max-warnings=0",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "commit": "cz",
    "release": "standard-version",
    "build": "npm run eslint && npm run prettier && rm -rf dist && tsc --build",
    "pub": "npm run build && npm run release && npm publish",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run eslint",
      "npm run prettier"
    ]
  },
  "keywords": [
    "typescript",
    "cli",
    "typescript 脚手架",
    "ts 脚手架",
    "tp-cli",
    "脚手架"
  ],
  "author": "Vane",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/mottzz87/tp-cli.git"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/node": "^16.11.36",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "husky": "^7.0.1",
    "prettier": "^2.3.2",
    "standard-version": "^9.3.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@types/inquirer": "^7.3.3",
    "@types/shelljs": "^0.8.9",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "axios": "^0.21.1",
    "bunyan": "^1.8.15",
    "chalk": "^4.1.2",
    "clear-console": "^1.1.0",
    "commander": "^8.1.0",
    "download-git-repo": "^3.0.2",
    "fs": "^0.0.1-security",
    "fs-extra": "^10.0.0",
    "ini": "^2.0.0",
    "inquirer": "^8.1.2",
    "log-symbols": "^4.1.0",
    "ora": "^5.4.1",
    "path": "^0.12.7",
    "semver": "^7.3.5",
    "shelljs": "^0.8.4",
    "yoo-hoo": "^1.3.1"
  }
}
